        <script nonce="e+fsQ3fEtE2XSpo8lEQ4/w==">
            function accept_cookies() {

                $.ajax({
                    method: "POST",
                    url: "/conf_gdpr/accept",
                    data: {
                        csrfmiddlewaretoken: csrftoken,
                    },
                }).done(function (data) {
                    console.log(data);
                    $("#cookie-bar").fadeOut();
                }).fail(function (jqXHR, textStatus) {
                    alert(textStatus);
                });
            }
            $("#accept_cookies_button").on("click", () => {
                accept_cookies();
            });
        </script>

    







